<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"  
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<!-- 컬렉션 setter 주입 -->
	<bean class="ex05.collection.Customer" id="cus">
	
		<!-- 컬렉션 String 타입 -->
		<property name="lists">
			<list>
				<value>홍길동</value>
				<value>이순신</value>
				<value>홍길자</value>
			</list>
		</property>
		<!-- 컬렉션 객체 타입 -->
		<property name="vo">
			<list>
				<bean class="ex05.collection.MemberVO" id="mVO1">
					<property name="name" value="홍길동"/>
					<property name="id" value="kkk123"/>
				</bean>
			</list>
		</property>				
		<!-- 맵 String -->
		<property name="maps">
			<map>
				<entry key="k1" value="홍길동"></entry>				
				<entry key="k2">
					<bean class="ex05.collection.MemberVO" id="mVO2">
						<property name="name" value="홍길자"/>
						<property name="id" value="aaa123"/>
					</bean>
				</entry>
			</map>
		</property>		
	</bean>
	
</beans>